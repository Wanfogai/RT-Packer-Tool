        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RT_Packer_Tool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="Main">
        <Menu x:Name="Menu">
            <MenuItem Header="Файл">
                <MenuItem Header="Открыть ресурс-файл"></MenuItem>
                <MenuItem Header="Сохранить как"></MenuItem>
                <MenuItem Header="Сохранить"></MenuItem>
                <Separator></Separator>
                <MenuItem Header="Выход"></MenuItem>
            </MenuItem>
            <MenuItem Header="Инструменты"></MenuItem>
            <MenuItem Header="Найстройки"></MenuItem>
        </Menu>

    </Grid>
</Window>
